on parseCsson(dir, item) {\n// \tif (getFileExt(item) === 'csson') {\n// \t\tlet object = csson(fs.readFileSync(path.join(dir, item), 'utf8'));\n\n// \t\treturn object;\n// \t}\n// }\n\nexport default function parseContent(dir, item) {\n\tlet result =\n\t\tparseJson(dir, item) ||\n\t\tparseMarkdown(dir, item) ||\n\t\tparseText(dir, item) ||\n\t\tparseYaml(dir, item) ||\n\t\tparseJson5(dir, item);\n\t// parseCsson(dir, item);\n\treturn result;\n}\n"},"lineCount":null}},"error":null,"hash":"c06663fdc26e81ad6b35486a91c3aed9","cacheData":{"env":{}}}